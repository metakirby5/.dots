// Place your settings in this file to overwrite the default settings
{
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [";"],
      "after": [":"]
    },
    {
      "before": [":"],
      "after": [";"]
    },
    {
      "before": ["0"],
      "after": ["^"]
    },
    {
      "before": ["^"],
      "after": ["0"]
    },
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": ["Q"],
      "after": ["@", "q"]
    },
    {
      "before": ["H"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["L"],
      "commands": ["workbench.action.nextEditor"]
    }
  ],
  "vim.leader": " ",
  "workbench.startupEditor": "newUntitledFile",
  "vim.statusBarColors.visualline": "",
  "explorer.confirmDelete": false,
  "editor.formatOnSave": true,
  "files.autoSave": "onFocusChange",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "emmet.excludeLanguages": ["markdown", "jsx"],
  "window.zoomLevel": 1,
  "editor.inlineSuggest.enabled": true,
  "update.showReleaseNotes": false,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "editor.accessibilitySupport": "off",
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "python.analysis.typeCheckingMode": "strict",
  "workbench.editor.showTabs": "single",
  "workbench.colorTheme": "Default Dark+",
  "java.autobuild.enabled": false,
  "editor.formatOnType": true,
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "[groovy]": {
    "editor.defaultFormatter": "NicolasVuillamy.vscode-groovy-lint"
  },
  "cmake.configureOnOpen": true,
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "editor.formatOnSaveMode": "modifications",
  "snyk.yesWelcomeNotification": false,
  "snyk.advanced.cliPath": "/Users/echan/Library/Application Support/snyk-ls/snyk-macos",
  "snyk.trustedFolders": [],
  "flake8.args": [
    "--max-line-length=88"
  ]
}
